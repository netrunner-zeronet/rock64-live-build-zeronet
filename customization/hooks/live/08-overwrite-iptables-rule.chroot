echo "I: Overwrite iptables rule [workaround for iptables-persistent package install]"
cat << EOF > /etc/iptables/rules.v4
# Generated by iptables-save v1.6.2 on Fri May  4 16:50:34 2018
*nat
:PREROUTING ACCEPT [0:0]
:INPUT ACCEPT [0:0]
:OUTPUT ACCEPT [1:595]
:POSTROUTING ACCEPT [1:595]
-A PREROUTING -p tcp -m tcp --dport 80 -j REDIRECT --to-ports 43110
COMMIT
# Completed on Fri May  4 16:50:34 2018
EOF
# Need to set iptables-legacy as iptables here as we use an older kernel
update-alternatives --set iptables /usr/sbin/iptables-legacy
